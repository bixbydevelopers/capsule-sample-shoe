capsule {
  id (example.shoe)
  version (2.1.2)
  format(3)
  targets {
    target (bixby-mobile-en-US)
  }
  runtime-version (7) {
    overrides {
      no-fallback-dialog-in-views (false)
    }
  }
  capsule-imports {
    import (viv.core) { as (core) }
    import (viv.money) { 
      as (money) version (2.22.43)
    }
  }
  store-countries {
    all
  }
  store-sections {
    section (Shopping)
  }
}